<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <conversionRule conversionWord="de"
                    class="com.chendashuai.desensitizetools.converter.DesensitizeConverter"/>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %de%n</pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="CONSOLE" />
    </root>

</configuration>